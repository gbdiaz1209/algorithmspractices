Algoritmo sin_titulo
	
	Definir a,b,resta, multip, divi como real
	Escribir "Ingrese el primer numero";
	Leer a;
	Escribir "Ingrese el segundo numero";
	Leer b;
	
	suma = a + b;
	resta = a - b;
	multip = a * b;
	divi = b/a
	
	Imprimir "resultados";
	Imprimir "Suma:",suma;
	Imprimir "Resta es:", resta;
	Imprimir "multiplicación es:", multip;
	Imprimir "La division es:",divi;
	
	

	
FinAlgoritmo
